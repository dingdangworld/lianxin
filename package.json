{
  "name": "lianxin",
  "version": "1.0.0",
  "description": "Lianxin social media platform",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon ./server.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "migrate:status": "sequelize db:migrate:status",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t user-service .",
    "docker:run": "docker run -p 3001:3001 user-service",
    "docker:test": "docker-compose -f docker-compose.test.yml up -d"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.3.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@elastic/elasticsearch": "^9.1.1",
    "ali-oss": "^6.23.0",
    "amqplib": "^0.10.3",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.0.3",
    "dotenv": "^16.1.4",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "ffmpeg-static": "5.1.0",
    "ffprobe-static": "^3.1.0",
    "file-type": "^21.0.0",
    "geolib": "^3.3.4",
    "heic-convert": "^2.1.0",
    "helmet": "^6.1.5",
    "image-size": "^2.0.2",
    "joi": "^17.9.2",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.10.30",
    "luxon": "^3.7.1",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "mysql2": "^3.3.3",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.7",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.31.1",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.2",
    "xss": "^1.0.14"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "social-media",
    "user-service",
    "microservice",
    "nodejs",
    "express",
    "mysql",
    "redis",
    "jwt",
    "china-compliance"
  ],
  "author": "Lianxin Development Team",
  "license": "PROPRIETARY"
}
